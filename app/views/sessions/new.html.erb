<h1>Login</h1>
<hr>

<div class="text-center">
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :username %><br>
    <%= f.text_field :username %><br><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br><br>
    <%= f.submit "Login", id: "login_button" %>
  <% end %>
</div>
