<h1>Image Library</h1>
<hr>
<div class="text-center">
  <%= link_to "Add Image", new_image_path, class: "btn btn-primary" %>
</div>
<hr>

<div class="text-center">
  <% @images.each do |image| %>
    <%= link_to image_tag(image.address), edit_image_path(image), class: "mosaic" %>
  <% end %>
</div>
